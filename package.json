{
  "name": "simple-react-form",
  "version": "1.0.0",
  "description": "A Simple Multi Step Form Application built using React and Redux",
  "main": "src/app.js",
  "scripts": {
    "start": "webpack-dev-server --config ./build-config/webpack.config.js --watch-poll",
    "c9start": "webpack-dev-server --config ./build-config/webpack.config.js --port $PORT --host $IP --public $C9_HOSTNAME --content-base build/",
    "build": "webpack -p --config ./build-config/webpack.prod.config.js",
    "deploy": "npm run build && npm run deploy-gh",
    "deploy-gh": "gh-pages -d build"
  },
  "repository": "git://github.com/kiranbandi/simple-react-form.git",
  "keywords": [
    "react",
    "form",
    "react-form",
    "redux-form"
  ],
  "author": "kiranbandi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kiranbandi/simple-react-form/issues"
  },
  "homepage": "https://github.com/kiranbandi/simple-react-form#readme",
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-amd": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "file-loader": "^0.10.0",
    "gh-pages": "^0.12.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.3.0",
    "react-hot-loader": "^1.3.1",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
